@keyframes spinner-loading
	0% 
		transform: rotate(1turn)
	to 
		transform: rotate(0deg)
		
@keyframes tinkle
	0%
		opacity: .7
	50%
		opacity: 1
	100%
		opacity: .8	

@keyframes fill
	0%
		width: 100%;

	100%
		width: 0%;
			
@keyframes swing
	0%
		transform: rotate(3deg)
	100%
		transform: rotate(-3deg)

@keyframes swong
	0%
		transform: rotate(-2deg)
	100%
		transform: rotate(5deg)


@keyframes fadeInRight
	from
		opacity: 0
		transform: translate3d(-50%, 0, 0)

	to
		opacity: 1
		transform: none

@keyframes fadeInLeft
	from
		opacity: 0
		transform: translate3d(10%, 0, 0)

	to
		opacity: 1
		transform: none

@keyframes fadeInDown
	from
		opacity: 0
		transform: translate3d(0, -30%, 0)

	to
		opacity: 1
		transform: none

@keyframes fadeOutUpBig
	from
		opacity: 1

	to
		opacity: 0
		transform: translate3d(0, -1000px, 0)

@keyframes fadeInUp
	from
		transform: translate3d(0, 10%, 0)

	to
		transform: none
		
@keyframes fadeInUpE
	from
		top: 0
		transform: translate3d(0, 10%, 0)

	to
		top: -3
		transform: none

@keyframes fadeIn
	from
		opacity: 0
		
	to
		opacity: 1

@keyframes bounceOut
	20%
		transform: scale3d(.9, .9, .9)

	50%, 55%
		opacity: 1
		transform: scale3d(1.1, 1.1, 1.1)

	to
		opacity: 0
		transform: scale3d(.3, .3, .3)

@keyframes bounceIn
	from, 20%, 40%, 60%, 80%, to
		animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000)

	0%
		opacity: 0
		transform: scale3d(.3, .3, .3)

	20%
		transform: scale3d(1.1, 1.1, 1.1)

	40%
		transform: scale3d(.9, .9, .9)

	60%
		opacity: 1
		transform: scale3d(1.03, 1.03, 1.03)

	80%
		transform: scale3d(.97, .97, .97)

	to
		opacity: 1
		transform: scale3d(1, 1, 1)

@keyframes tada
	from
		transform: scale3d(1, 1, 1)

	10%, 20%
		transform: scale3d(.9, .9, .9) rotate3d(0, 0, 1, -3deg)

	30%, 50%, 70%, 90%
		transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, 3deg)

	40%, 60%, 80%
		transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, -3deg)

	to
		transform: scale3d(1, 1, 1)

@keyframes char
	0%
		background-position: center 30em
	
	100%
		background-position: center top
		opacity: 1

@keyframes glows
	0%
		filter: drop-shadow(0 1em 0.6em rgba(254, 202, 50, 0));

	50%
		filter: drop-shadow(0 1em 0.6em rgba(254, 202, 50, 1));
	
	100%
		filter: drop-shadow(0 1em 0.6em rgba(254, 202, 50, 0));

@keyframes glowsBlue
	0%
		filter: drop-shadow(0 0 0.6em rgba(109, 161, 255, 0));

	50%
		filter: drop-shadow(0 0 0.8em rgba(109, 161, 255, 1));
	
	100%
		filter: drop-shadow(0 0 0.6em rgba(109, 161, 255, 0));
		
@keyframes shairs
	0%
		background-size: 64em auto
		background-position: center 0
		
	50% 
		background-position: center -36em
		
	100%
		background-size: 118em auto;
		background-position: center -46em;
