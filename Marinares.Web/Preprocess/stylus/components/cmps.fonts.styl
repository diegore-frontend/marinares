@font-face
	font-family: 'ms-icons'
	src: url('../../fonts/ms-icons.eot');
	src: url('../../fonts/ms-icons.eot#iefix') format('embedded-opentype'),
		url('../../fonts/ms-icons.ttf?1ksir') format('truetype'),
		url('../../fonts/ms-icons.woff?1ksir') format('woff'),
		url('../../fonts/ms-icons.svg?1ksir#ms-icons') format('svg');
	font-weight: normal;
	font-style: normal;

[class^="icon-ms-"], [class*=" icon-ms-"]
	&:before
		font-family: 'ms-icons' !important
		speak: none
		font-style: normal
		font-weight: normal
		font-variant: normal
		text-transform: none
		line-height: 1
		-webkit-font-smoothing: antialiased
		-moz-osx-font-smoothing: grayscale


